{
  "short_name": "Bookmarker Extension",
  "name": "Bookmarker Extension",
  "icons": {
    "48": "icon.png",
    "128": "icon128.png"
  },
  "permissions": ["http://localhost/*", "activeTab", "tabs"],
  "commands": {
    "open_manager": {
      "description": "Open bookmarker server main page"
    },
    "add_bookmark": {
      "description": "Bookmark current page"
    }
  },
  "background": {
    "scripts": ["scripts/background/index.js"]
  },
  "manifest_version": 2,
  "version": "0.2.7",
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Bookmarker"
  }
}
