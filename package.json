{
  "name": "bookmarker-extension",
  "version": "0.2.2",
  "private": true,
  "dependencies": {
    "formik": "2.2.0",
    "prop-types": "15.7.2",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-icons": "3.11.0",
    "styled-components": "5.2.0",
    "yup": "0.29.3",
    "snyk": "1.413.3"
  },
  "devDependencies": {
    "eslint-config-standard": "14.1.1",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.21.4",
    "eslint-plugin-standard": "4.0.1",
    "react-scripts": "3.4.3",
    "web-ext": "5.2.0",
    "webextension-polyfill": "0.6.0",
    "webpack-cli": "4.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build_scripts": "webpack --config webpack.config.js",
    "build": "INLINE_RUNTIME_CHUNK=false react-scripts build && npm run build_scripts",
    "package": "web-ext build --config=webext.config.js",
    "complete": "npm run build && npm run package",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "snyk": true
}
